{
  "name": "extension",
  "displayName": "extension",
  "description": "vscode ext base kit",
  "version": "1.0.1",
  "publisher": "yongliang",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "activationEvents": [],
  "main": "./out/extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "panel-view-container.show",
        "title": "Build & Deploy",
        "category": "vscode-webview-example"
      },
      {
        "command": "project-helper.build_deploy",
        "title": "Build & Deploy"
      },
      {
        "command": "project-helper.add_git_submodule",
        "title": "Add Submodule"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "project-helper.build_deploy",
          "group": "navigation",
          "when": "resourceLangId == json"
        },
        {
          "command": "panel-view-container.show",
          "group": "navigation",
          "when": "resourceLangId == vue"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "sidebar-view",
          "title": "人生如逆旅，我亦是行人。",
          "icon": "assets/workspace.svg"
        }
      ]
    },
    "views": {
      "sidebar-view": [
        {
          "id": "sidebar-view-container",
          "type": "webview",
          "name": ""
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run build-rollup",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "clear": "rimraf ./out && rimraf ./node_modules",
    "clear:out": "rimraf ./out/extension",
    "build-rollup": "pnpm run clear:out && rollup -c",
    "package": "pnpm vsce package  --no-dependencies"
  },
  "dependencies": {
    "@jsonrpc-rx/server": "^0.2.1",
    "html-modifier": "^1.2.0",
    "ssh2-promise": "^1.0.3",
    "ssh2-streams": "^0.4.10"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/ssh2-streams": "^0.1.12",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "rollup": "^4.34.8",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "@types/ssh2": "^1.15.4"
  }
}